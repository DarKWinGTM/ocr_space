# !/usr/bin/env ruby
require 'colorize'
require_relative "../lib/ocr_space"
require_relative "../lib/ocr_space/convert"


if ARGV.empty?
  puts "You need to mention the image path".colorize(:red)
else
    resource = OcrSpace::Resource.new
    result = resource.convert file: ARGV[0]
    puts result
end
